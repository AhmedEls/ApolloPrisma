{
    "version": 2,
    "env": {},
    "builds": [{
        "src": "dist/server.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["dist/**"]
        }
    }],
    "routes": [{
            "src": "/(.*)",
            "dest": "dist/server.js"
        },
        {
            "src": "/",
            "dest": "ui/build/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "ui/build/$1"
        }
    ]
}